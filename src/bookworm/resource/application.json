{
  "application": {
    "name": "BookwormLite",
    "version": "3.1",
    "devMode": true,
    "dbHost": "thot-container"
  },
  "dobby": {
    "staticContent": {
      "directory": "static/out"
    },
    "session": {
      "age": 5,
      "store": "hades.session.HadesSessionStore"
    },
    "port": 2711,
    "threads": 10,
    "logLevel": "DEBUG",
    "mode": "http"
  },
  "hades": {
    "login": {
      "maxAttempts": 5,
      "redirect":{
        "success": "/",
        "successAdmin": "/hades"
      }
    },
    "unauthorizedRedirectTarget": "/hades/login",
    "unauthorizedRedirectPaths": [
      "/",
      "/index.html",
      "/students",
      "/students/",
      "/students/index.html",
      "/books",
      "/books/",
      "/books/index.html",
      "/evaluation",
      "/evaluation/",
      "/evaluation/index.html",
      "/settings",
      "/settings/",
      "/settings/index.html"
    ]
  }
}